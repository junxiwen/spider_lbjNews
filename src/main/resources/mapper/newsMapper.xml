<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hyanzz.news.spider.dao.NewsMapper">


    <insert id="save">
        insert into news(id,`type`,indextitle,filename,title,shortTitle,lable,createtime,thumbnail,url,`date`)
        values (#{id},#{type},#{indextitle},#{filename},#{title},#{shortTitle},#{lable},#{createtime},#{thumbnail},#{url},#{date})
    </insert>
</mapper>
